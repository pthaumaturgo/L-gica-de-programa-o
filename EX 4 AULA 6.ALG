Algoritmo "EX 4 AULA 6"
// SITUAÇÃO 4: pagar impostos (porcentagem) - imposto 60%
// x.60/100
Var
      Preco, imposto: real

Inicio
      Escreval ("Qual o valor dos produtos?")
      Leia (preco)
      Imposto <- (preco * 60) / 100
      Escreval ("O valor dos impostos é de U$ ", imposto:5:2)

Fimalgoritmo